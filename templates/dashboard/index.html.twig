{% extends 'base.html.twig' %}

{% block title %}Anikura Dashboard{% endblock %}

{% block body %}
  <h1>Your uploads</h1>

  {% if userContent is empty %}
    <div class="alert alert-secondary">
      You haven't uploaded any content yet!
    </div>
  {% else %}
  <ul>
    {% for item in userContent %}
      <li>{{ item.name }}</li>
    {% endfor %}
  </ul>
  {% endif %}
{% endblock %}
